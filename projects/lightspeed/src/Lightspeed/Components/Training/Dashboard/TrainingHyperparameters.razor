<div class="row">
	<!-- Generic Hyperparameters -->
	<div class="col-6">
		<h3 class="text-info text-center">
			Generic Hyperparameters
		</h3>
		<table class="table text-light">
			<thead>
				<tr>
					<th scope="col">Name</th>
					<th scope="col">Value</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var (key, value) in GenericHyperparameters.Parameters)
				{
					<tr>
						<td>@key</td>
						<td>@value</td>
					</tr>
				}
			</tbody>
		</table>
	</div>

	<!-- Model-Specific Hyperparameters -->
	<div class="col-6">
		<h3 class="text-info text-center">
			Model-Specific Hyperparameters
		</h3>
		<table class="table text-light">
			<thead>
				<tr>
					<th scope="col">Name</th>
					<th scope="col">Value</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var (key, value) in ModelHyperparameters)
				{
					<tr>
						<td>@key</td>
						<td>@value</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>
